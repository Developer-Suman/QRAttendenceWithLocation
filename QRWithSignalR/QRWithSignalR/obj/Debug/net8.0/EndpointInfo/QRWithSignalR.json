{
  "openapi": "3.0.1",
  "info": {
    "title": "NETRAVERSE API",
    "version": "V1"
  },
  "paths": {
    "/api/Purchase/add": {
      "post": {
        "tags": [
          "Purchase"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/PurchaseDTOs"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/PurchaseDTOs"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/PurchaseDTOs"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/Purchase/test": {
      "get": {
        "tags": [
          "Purchase"
        ],
        "parameters": [
          {
            "name": "abc",
            "in": "query",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/Purchase/MarkAttendance": {
      "post": {
        "tags": [
          "Purchase"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/AttandanceDTOs"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/AttandanceDTOs"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/AttandanceDTOs"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "AttandanceDTOs": {
        "type": "object",
        "properties": {
          "wifiIpAddress": {
            "type": "string",
            "nullable": true
          },
          "latitude": {
            "type": "number",
            "format": "double",
            "nullable": true
          },
          "longitude": {
            "type": "number",
            "format": "double",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "PurchaseDTOs": {
        "type": "object",
        "properties": {
          "itemName": {
            "type": "string",
            "nullable": true
          },
          "price": {
            "type": "number",
            "format": "double"
          },
          "qty": {
            "type": "integer",
            "format": "int32"
          }
        },
        "additionalProperties": false
      }
    },
    "securitySchemes": {
      "Bearer": {
        "type": "http",
        "description": "Please enter a valid token",
        "scheme": "Bearer",
        "bearerFormat": "JWT"
      }
    }
  },
  "security": [
    {
      "Bearer": [ ]
    }
  ]
}